{
  "name": "itgm",
  "version": "0.0.1",
  "description": "ITGM app",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "build": "rm -Rf dist && NODE_ENV=production parcel build index.html",
    "size": "size-limit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Mikhail Poluboyarinov <mikle.sol@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.20.1",
    "history": "^4.9.0",
    "qs": "^6.7.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "storeon": "^0.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "size-limit": "^1.3.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "size-limit": [
    {
      "webpack": false,
      "path": "dist/*.js"
    }
  ]
}
